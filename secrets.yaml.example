# ESP32-S3-Box3 Secrets Template
# Copy this file to secrets.yaml and fill in your values:
#   cp secrets.yaml.example secrets.yaml

# WiFi credentials (required)
wifi_ssid: "YOUR_WIFI_SSID"
wifi_password: "YOUR_WIFI_PASSWORD"

# Optional: API encryption key for Home Assistant communication
# Generate with: openssl rand -base64 32
# api_encryption_key: "your-base64-key-here"

# Optional: OTA password for over-the-air updates
# ota_password: "your-ota-password"
